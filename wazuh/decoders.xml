<!-- Decode execution log -->
<decoder name="infostealer-exec">
  <prematch>infostealer_sim.py</prematch>
</decoder>

<!-- Decode OAuth token pattern -->
<decoder name="browser-token">
  <prematch>ya29\.</prematch>
</decoder>

<!-- Decode JSON network logs -->
<decoder name="c2-exfil">
  <prematch>"destination_port":</prematch>
  <json>
    <field name="destination_port">destination_port</field>
    <field name="destination_host">destination_host</field>
  </json>
</decoder>
